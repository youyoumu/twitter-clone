<%= render  partial: "tweet_error", locals: {tweet: tweet} %>
<%= form_with model: tweet, local: true do |f|%>
  <%= f.text_area :content, class: "form-control mb-2" %>
  <div class="input-group">
    <%= f.file_field :tweet_pic, class: "form-control" %>
    <%= f.submit "Tweet", class: "btn btn-primary" %>
  </div>
<% end %>